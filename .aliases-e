[ -x /usr/bin/eopkg ] || return

alias ac='sudo eopkg delete-cache'
alias ai='sudo eopkg install'
alias al="eopkg list-installed | tr -s ' ' | grep -i"
alias an='eopkg info'
alias ao='sudo eopkg remove-orphans'
alias ar='sudo eopkg autoremove'
alias as="eopkg list-available | tr -s ' ' | grep -i"
alias au='sudo eopkg upgrade'
